{
  "name": "minecraft-marketplace-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/llgava/minecraft-marketplace-api.git",
  "author": "llgava <llgavamt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./build/src/index.js",
    "build": "babel ./src  --extensions \".js,.ts\" --out-dir ./build/src --copy-files --no-copy-ignored && clear && echo \"🏗️  Compiling...\"",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "clear && echo \"🃏  Running Jest...\n \" && jest",
    "test:coverage": "clear && echo \"🃏  Running Jest with Coverage...\n \" && jest --coverage"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "express": "^4.17.1",
    "mongoose": "^5.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/supertest": "^2.0.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
